SPAWNPOINTS_NUMBER=2
CITIES_NUMBER=5
//set (choose) invasion direction
invasion_direction=choose("UP","LEFT","RIGHT","DOWN")
spawn_x=0
spawn_y=0;


//create monsters spawnpoints
for(i=0;i&lt;SPAWNPOINTS_NUMBER;i++){
   switch(invasion_direction){
   //TODO: this is quick and dirty, write something beter!
    case "UP":
        spawn_x=random_range(0,room_width)
        spawn_y=random_range(-256,0)
        break;
    case "LEFT":
        spawn_x=random_range(-256,0)
        spawn_y=random_range(0,room_height)
        break;
    case "DOWN":
        spawn_x=random_range(0,room_width)
        spawn_y=random_range(room_height,room_height+256)
        break;
    case "RIGHT":
        spawn_x=random_range(room_width, room_width+256)
        spawn_y=random_range(0,room_height)
        break;
    } 
    instance_create(spawn_x,spawn_y,obj_Colony)
}

//create cities and initiate cities list
for(i=0;i&lt;CITIES_NUMBER;i++){
   switch(invasion_direction){
   //TODO: this is quick and dirty, write something beter!
    case "UP":
        spawn_x=random_range(0,room_width)
        spawn_y=random_range(room_height/4,room_height)
        break;
    case "LEFT":
        spawn_x=random_range(room_width/4,room_width)
        spawn_y=random_range(0,room_height)
        break;
    case "DOWN":
        spawn_x=random_range(0,room_width)
        spawn_y=random_range(0,room_height*0.75)
        break;
    case "RIGHT":
        spawn_x=random_range(0, room_width*0.75)
        spawn_y=random_range(0,room_height)
        break;
    } 
    city=instance_create(spawn_x,spawn_y,obj_City)
    //cities[i]=(instance_find(obj_City,i))
}


for(i=0;i&lt;instance_number(obj_City);i++){
    cities[i]=(instance_find(obj_City,i))
}
