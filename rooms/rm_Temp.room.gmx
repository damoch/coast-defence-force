//might comment this line for debug
randomize();

//room creation code
SPAWNPOINTS_NUMBER=5
CITIES_NUMBER=7
evac_sprite=spr_TempEvacPoint
infa_sprite=spr_TempUnitSpawner
//set (choose) invasion direction
invasion_direction=choose("UP","LEFT","DOWN")
spawn_x=0
spawn_y=0;

lowest_x=0
highest_x=0

sdads=0

lowest_y=0
highest_y=0

//evacuation point position
evac_point_x=0
evac_point_y=0

//infantrySpawner position
infa_spawn_x=0
infa_spawn_y=0

//set irandom ranges
switch(invasion_direction){
    case "UP":
        lowest_x=0
        highest_x=room_width
        lowest_y=-256
        highest_y=0
        
        evac_point_x=irandom(room_width)
        evac_point_y=room_height-sprite_get_height(evac_sprite)
                
        infa_spawn_x=irandom(room_width)
        infa_spawn_y=room_height-sprite_get_height(infa_sprite)/2
        break;
    case "LEFT":
        lowest_x=-256
        highest_x=0
        lowest_y=0
        highest_y=room_height
        
        evac_point_x=room_width-sprite_get_width(evac_sprite)/2
        evac_point_y=irandom(room_height)
        
        infa_spawn_x=room_width-sprite_get_width(infa_sprite)
        infa_spawn_y=irandom(room_height)
        break;
    case "DOWN":
        lowest_x=0
        highest_x=room_width
        lowest_y=room_height
        highest_y=room_height+256
        
        evac_point_x=irandom(room_width)
        evac_point_y=sprite_get_height(evac_sprite)
        
        infa_spawn_x=irandom(room_width)
        infa_spawn_y=sprite_get_height(infa_sprite)/2
        break;
    case "RIGHT":
        lowest_x=room_width
        highest_x=room_width+256
        lowest_y=0
        highest_y=room_height
        
        evac_point_x=sprite_get_width(evac_sprite)
        evac_point_y=irandom(room_height)
        
        infa_spawn_x=sprite_get_width(infa_sprite)/2
        infa_spawn_y=irandom(room_height)
        break;
} 


//create monsters spawnpoints
for(i=0;i&lt;SPAWNPOINTS_NUMBER;i++){
    spawn_x=irandom_range(lowest_x,highest_x)
    spawn_y=irandom_range(lowest_y,highest_y)
    instance_create(spawn_x,spawn_y,obj_Colony)
}
//set lowest and highest ranges

switch(invasion_direction){
    case "UP":
        lowest_x=0
        highest_x=room_width
        lowest_y=room_height*0.25
        highest_y=room_height
        break;
    case "LEFT":
        lowest_x=room_width*0.25
        highest_x=room_width
        lowest_y=0
        highest_y=room_height
        break;
    case "DOWN":
        lowest_x=0
        highest_x=room_width
        lowest_y=0
        highest_y=room_height*0.75
        break;
    case "RIGHT":
        lowest_x=0
        highest_x=room_width*0.75
        lowest_y=0
        highest_y=room_height
        break;
} 

//create evac point
instance_create(evac_point_x,evac_point_y,obj_EvacuationPoint)

//create infantry spawn
//instance_create(infa_spawn_x,infa_spawn_y,obj_UnitSpawner)

//create cities
for(i=0;i&lt;CITIES_NUMBER;i++){
    spawn_x=irandom_range(lowest_x,highest_x)
    spawn_y=irandom_range(lowest_y,highest_y)
    if(!(place_meeting(spawn_x,spawn_y,obj_City))){
        city=instance_create(spawn_x,spawn_y,obj_City)
        }
    else{
        CITIES_NUMBER++
        }
}
//setting buttons

instance_create(40,160,obj_UnitSpawner)
instance_create(40,256,obj_SpecialForcesSpawner)
instance_create(40,352,obj_TankSpawner)
instance_create(40,448,gui_HeloSupportButton)
instance_create(40,544,gui_AirStrikeButton)




